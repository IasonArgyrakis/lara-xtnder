{
    "name": "iasonargyrakis/lara-xtnder",
    "description": "",
    "keywords": [
        "iasonargyrakis",
        "lara-xtnder"
    ],
    "homepage": "https://github.com/iasonargyrakis/lara-xtnder",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Iason Argyrakis",
            "email": "jasonargirakis@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "IasonArgyrakis\\LaraXtnder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IasonArgyrakis\\LaraXtnder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "IasonArgyrakis\\LaraXtnder\\LaraXtnderServiceProvider"
            ],
            "aliases": {
                "LaraXtnder": "IasonArgyrakis\\LaraXtnder\\LaraXtnderFacade"
            }
        }
    },
     "minimum-stability": "dev",
    "prefer-stable": true
}
